{
    "defaults": {
        "later_version_ok": true,
        "read_stderr_instead_of_stdout": false,
        "version_regex_pattern": "((\\d+)(\\.(\\d+))+)"
    },
    "requirements": [
        {
            "name": "Bash",
            "min_version": "3.2",
            "command": [
                "bash",
                "--version"
            ]
        },
        {
            "name": "Binutils",
            "min_version": "2.25",
            "command": [
                "ld",
                "--version"
            ]
        },
        {
            "name": "Bison",
            "min_version": "2.7",
            "command": [
                "bison",
                "--version"
            ]
        },
        {
            "name": "Bzip2",
            "min_version": "1.0.4",
            "command": [
                "bzip2",
                "--version"
            ],
            "read_stderr_instead_of_stdout": true
        },
        {
            "name": "Coreutils",
            "min_version": "6.9",
            "command": [
                "chown",
                "--version"
            ]
        },
        {
            "name": "Diffutils",
            "min_version": "2.8.1",
            "command": [
                "diff",
                "--version"
            ]
        },
        {
            "name": "flex",
            "min_version": "2.6.4",
            "command": [
                "flex",
                "--version"
            ]
        },
        {
            "name": "Findutils",
            "min_version": "4.3.21",
            "command": [
                "find",
                "--version"
            ]
        },
        {
            "name": "Gawk",
            "min_version": "4.0.1",
            "command": [
                "gawk",
                "--version"
            ]
        },
        {
            "name": "GCC",
            "min_version": "6.2",
            "command": [
                "gcc",
                "--version"
            ]
        },
        {
            "name": "Glibc",
            "min_version": "2.11",
            "command": [
                "ldd",
                "--version"
            ]
        },
        {
            "name": "Grep",
            "min_version": "2.5.1a",
            "command": [
                "grep",
                "--version"
            ],
            "version_regex_pattern": "((\\d+)(\\.(\\d+))+[a-z]?)"
        },
        {
            "name": "Gzip",
            "min_version": "1.3.12",
            "command": [
                "gzip",
                "--version"
            ]
        },
        {
            "name": "Linux kernel",
            "min_version": "3.2",
            "command": [
                "cat",
                "/proc/version"
            ]
        },
        {
            "name": "M4",
            "min_version": "1.4.10",
            "command": [
                "m4",
                "--version"
            ]
        },
        {
            "name": "Make",
            "min_version": "4.0",
            "command": [
                "make",
                "--version"
            ]
        },
        {
            "name": "Patch",
            "min_version": "2.5.4",
            "command": [
                "patch",
                "--version"
            ]
        },
        {
            "name": "Perl",
            "min_version": "5.8.8",
            "command": [
                "perl",
                "--version"
            ]
        },
        {
            "name": "Python",
            "min_version": "3.7",
            "command": [
                "python3.7",
                "--version"
            ],
            "later_version_ok": false
        },
        {
            "name": "Sed",
            "min_version": "4.1.5",
            "command": [
                "sed",
                "--version"
            ]
        },
        {
            "name": "Tar",
            "min_version": "1.22",
            "command": [
                "tar",
                "--version"
            ]
        },
        {
            "name": "Texinfo",
            "min_version": "4.7",
            "command": [
                "makeinfo",
                "--version"
            ]
        },
        {
            "name": "XZ",
            "min_version": "5.0.0",
            "command": [
                "xz",
                "--version"
            ]
        }
    ]
}
